{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://vega.github.io/editor/data/movies.json"},
    "mark": "bar",
    "encoding": {
      "x" : {
        "field": "IMDB Rating",
        "type": "quantitative",
        "bin" : true
      },
      "y" : {
        "aggregate": "mean",
        "field" : "US Gross"
      }
    }
  }